<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background Color -->
    <item android:drawable="@android:color/white" />

    <!-- Border -->
    <item>
        <shape
            android:shape="rectangle">
            <stroke
                android:width="1dp"
            android:color="#CCCCCC" />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
</layer-list>
